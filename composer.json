	"scripts": {
		"pre-install-cmd": [
			"Tiki\\Composer\\ThemeInstaller::setup"
		],
		"post-install-cmd": [
			"Tiki\\Composer\\BootstrapCompiler::build",
			"Tiki\\Composer\\CleanVendors::clean"
		],
		"pre-update-cmd": [
			"Tiki\\Composer\\ThemeInstaller::setup"
		],
		"post-update-cmd": [
			"Tiki\\Composer\\BootstrapCompiler::build",
			"Tiki\\Composer\\CleanVendors::clean"
		]
	},
